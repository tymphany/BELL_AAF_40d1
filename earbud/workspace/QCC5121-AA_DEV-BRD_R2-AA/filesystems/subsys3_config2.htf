file=audio2

# Select PIOs for I2S interface 1: SCK, WS, MCLK, SD_OUT, SD_IN.
Pcm1PioConfig = [3E 3D 36 3A 38]

DigMic0G722FilterEnable = true
DigMic0G722FirEnable = true
#ENABLE_TYM_PLATFORM
DigMic0ChanSwap = true
DigMic0ClockRate = 4000
#ENABLE_TYM_PLATFORM
DigMic0PioConfig = [02 03]

DigMic1G722FilterEnable = true
DigMic1G722FirEnable = true
DigMic1ChanSwap = false
DigMic1ClockRate = 4000
DigMic1PioConfig = [ff ff]

DigMic2G722FilterEnable = true
DigMic2G722FirEnable = true
DigMic2ChanSwap = false
DigMic2ClockRate = 4000
DigMic2PioConfig = [ff ff]

#ENABLE_TYM_PLATFORM ABAmplifier
#Codec0UseClassABAmplifier = false
Codec0UseClassABAmplifier = true

#Maximum Time is 4294967295 ms
#ENABLE_TYM_PLATFORM,dac 30min
CodecOutputDisableTimeout = 1800000000
#CodecOutputDisableTimeout = 28000000

#Uncomment the following line if using ANC
#IsAncIirFilter16k = true

AncEnableDelay = 300
#ENABLE_TYM_PLATFORM.Qualcomm suggest for earbud no sound
ProfilerEnable=true
